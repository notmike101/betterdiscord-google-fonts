{
  "name": "google-fonts",
  "version": "2.2.0",
  "description": "Injects Google Fonts into Discord",
  "main": "./src/main.ts",
  "scripts": {
    "build": "node ./scripts/build.mjs",
    "build:dev": "node ./scripts/build.mjs --dev",
    "watch": "node ./scripts/build.mjs --watch",
    "watch:dev": "node ./scripts/build.mjs --watch --dev",
    "update-fonts": "node ./scripts/update-google-fonts.mjs",
    "install-plugin": "node --experimental-json-modules ./scripts/install.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notmike101/betterdiscord-google-fonts.git"
  },
  "author": {
    "name": "Mike Orozco",
    "email": "me@mikeorozco.dev",
    "url": "https://mikeorozco.dev"
  },
  "license": "LICENSE.txt",
  "bugs": {
    "url": "https://github.com/notmike101/betterdiscord-google-fonts/issues"
  },
  "homepage": "https://github.com/notmike101/betterdiscord-google-fonts",
  "dependencies": {
    "@types/bandagedbd__bdapi": "^0.3.4",
    "betterdiscord-plugin-libs": "^1.2.0",
    "dotenv": "^16.0.1",
    "esbuild": "^0.14.38",
    "esbuild-sass-plugin": "^2.2.6",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
